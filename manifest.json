
{
  "manifest_version": 2,
  "name": "Form State Remembrancer",
  "version": "1.1.0",
  "description": "Remembers and restores the state of form inputs on a page. Don't you never fill same form twice....again!",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
    "browser_specific_settings": {
    "gecko": {
      "id": "@form-remembrancer",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
   "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icons/icon-32.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
